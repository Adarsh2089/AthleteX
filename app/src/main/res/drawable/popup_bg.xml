<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="@color/backgroundFill"
                android:endColor="@color/backgroundFill"
                android:angle="45" />
            <corners android:radius="25dp"/>
            <padding
                android:left="16dp"
                android:right="16dp"
                android:top="16dp"
                android:bottom="16dp"/>
        </shape>
    </item>
</layer-list>
